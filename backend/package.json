{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "",
  "main": "main.js",
  "scripts": {
    "start": "node main.js",
    "dev": "nodemon -r dotenv/config main.js",
    "db-create": "npx sequelize-cli db:create",
    "db-drop": "node db/scripts/terminateConnections.js && npx sequelize-cli db:drop",
    "regen-migrations": "node db/scripts/deleteMigrations.js && npx sequelize-mig migration:make -n init",
    "migrations": "npx sequelize-cli db:migrate",
    "db-seed": "npx sequelize-cli db:seed:all",
    "db-seed-undo": "npx sequelize-cli db:seed:undo",
    "db-all": "npm run db-drop && npm run db-create && npm run regen-migrations && npm run migrations && npm run db-seed"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "ms": "^2.1.3",
    "nodemon": "^3.1.10",
    "pg": "^8.16.3",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "sequelize-mig": "^3.1.3"
  }
}
